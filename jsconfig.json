{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "checkJs": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.js",
    "**/*.vue"
  ],
  "exclude": [
    "node_modules",
    "unpackage",
    "dist"
  ]
}